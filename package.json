{
  "name": "station-ui",
  "files": [
    "dist"
  ],
  "main": "dist/station-ui.umd.cjs",
  "module": "dist/station-ui.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/station-ui.js",
      "require": "./dist/station-ui.umd.cjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-dictionary": "style-dictionary build --config ./style-dictionary/config.json",
    "build": "npm run build-dictionary && tsc && vite build && cp src/index.d.ts dist/index.d.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:native": "storybook dev -p 7007 --config-dir .storybook",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.2",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/react-native": "^6.5.6",
    "@terra-money/terra-utils": "^1.1.0",
    "@types/styled-components": "^5.1.26",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-modal": "^3.16.1",
    "react-native": "^0.72.4",
    "styled-components": "^6.0.7",
    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.1.1",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.1.1",
    "@storybook/react": "^7.1.1",
    "@storybook/react-vite": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-modal": "^3.16.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "sass": "^1.64.1",
    "storybook": "^7.1.1",
    "storybook-addon-themes": "^6.1.0",
    "style-dictionary": "^3.8.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
